uptime_updatedata()
{
   UPDATA="$*"
}

uptime_www()
{
    UP="$*"
    UPDAYS=$(echo $UP | awk -F" days" '{print $1}')
    UPMINUTES=$(echo $UP | cut -d":" -f2)
    UPHOURS=$(echo $UP | awk -F"days, " '{print $2}' | cut -d":" -f1)

    PSORTCUSTOM=$(printf "%d%.2d%.2d" $UPDAYS $UPHOURS $UPMINUTES)

    if (( $UPDAYS <= 5 )); then
        PSTATETYPE="highuptime:false lowuptime:true"
    elif (( $UPDAYS >= 100 )); then
        PSTATETYPE="highuptime:true lowuptime:false"
    else
        PSTATETYPE="highuptime:false lowuptime:false"
    fi

    PVALUE="$*"
}

getcolumninfo()
{
    WEBINTERFACE=true
    COLNAME=Uptime
}
