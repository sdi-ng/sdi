function updatedata()
{
   UPDATA="$*"
}

function www()
{
    UP="$*"
    UPDAYS=$(awk -F" days" '{print $1}' <<< $UP)
    UPMINUTES=$(cut -d":" -f2 <<< $UP)
    UPHOURS=$(awk -F"days, " '{print $2}' <<< $UP | cut -d":" -f1)

    PSORTCUSTOM=$(printf "%d%.2d%.2d" $UPDAYS $UPHOURS $UPMINUTES)
    PVALUE="$*"
}

function getcolumninfo()
{
    WEBINTERFACE=true
    COLNAME=Uptime
}
