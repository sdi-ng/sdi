# 035-last-contact

function getcolumn()
{
    LAST="$(tail -n 1 $DATA)"
    if grep -q "^ssh" <<< $LAST; then
        printf "<td>$(awk -F':' '{print $NF}' <<< $LAST)</td>"
    else
        printf "<td>$(cut -d" " -f1,2,3 <<< $LAST)</td>"
    fi
}
function getcolumnname()
{
    printf "&Uacute;ltimo Contato"
}

