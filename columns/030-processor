# 030-processor

function getcolumn()
{
    PROCESSORLN=$(grep "PROCESSOR" $DATA|tail -1)
    if test -n "$PROCESSORLN"; then
        PROCESSOR=$(cut -d'+' -f2 <<< $PROCESSORLN|awk '{print $1" "$2}')
        NoPROCESSOR=$(cut -d'+' -f3 <<< $PROCESSORLN)
        printf "<td>$PROCESSOR</td>"
    else
        printf "<td></td>"
    fi
}
function getcolumnname()
{
    printf "Processador"
}

