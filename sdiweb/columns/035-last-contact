# 035-last-contact

function getcolumn()
{
    LASTCONTACT=$(grep "SDI+LAST+CONTACT" $DATA|tail -1)
    LASTLINE=$(tail -n1 $DATA)
    if test "$LASTLINE" != "$LASTCONTACT"; then
        LASTERROR="$LASTLINE"
    fi
    LASTCONTACTTIME=$(cut -d' ' -f1,2,3 <<< $LASTCONTACT)
    test -n "$LASTCONTACTTIME" && LASTCONTACTTIME="$LASTCONTACTTIME<br/>"
    printf "<td>$LASTCONTACTTIME$LASTERROR</td>"
}
function getcolumnname()
{
    printf "<td>&Uacute;ltimo Contato/Erro</td>"
}

