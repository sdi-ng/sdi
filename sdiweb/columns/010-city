# 010-city

function getcolumn()
{
    CITY=$(awk '{print $NF}' <<< $1|cut -d':' -f2|tr '_' ' ')
    CITY=$(sed 's/^\(.\)\(.*\)/\1\L\2/' <<< $CITY)
    CITY=$(sed 's/ \(.\)\(.*\)/ \u\1\L\2/g' <<< $CITY)
    printf "<td>$CITY</td>"
}
function getcolumnname()
{
    printf "<td>Cidade</td>"
}

