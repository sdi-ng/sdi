# 005-nre

function getcolumn()
{
    NRE=$(awk '{print $NF}' <<< $1|cut -d':' -f1|cut -d';' -f2)
    NRE=$(tr "_" " " <<< $NRE)
    NRE=$(sed 's/^\(.\)\(.*\)/\1\L\2/' <<< $NRE)
    NRE=$(sed -e 's/ \(.\)\(.*\)/ \u\1\L\2/g' <<< $NRE)
    printf "<td>$NRE</td>"
}
function getcolumnname()
{
    printf "<td>NRE</td>"
}

